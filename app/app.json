{
  "expo": {
    "name": "CoolFitness",
    "slug": "coolfitness",
    "version": "0.1.0",
    "scheme": [
      "coolfitness"
    ],
    "newArchEnabled": true,
    "android": {
      "package": "com.coolfitness.app",
      "versionCode": 1,
      "permissions": [
        "ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mascot/default.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90489b54-2458-4406-a394-e31059a52f0f"
      }
    },
    "ios": {
      "bundleIdentifier": "com.coolfitness.app"
    }
  }
}
